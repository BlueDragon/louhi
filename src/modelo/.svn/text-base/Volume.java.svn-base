package modelo;

/**
 *
 * @author Oswaldo
 */
public class Volume implements Node{
    public String vol;

    public Volume() {
        
    }

    public Volume(String aString) {
        this.vol=aString;
    }

    public String getVolume() {
        return vol;
    }

    public void setVolume(String vol) {
        this.vol = vol;
    }

    @Override
    public String toString() {
        return this.vol;
    }

    
}
